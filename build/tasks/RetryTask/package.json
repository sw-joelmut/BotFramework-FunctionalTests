{
  "name": "retry-task",
  "version": "0.0.1",
  "description": "Task wrapper for retry other Azure DevOps tasks.",
  "main": "dist/retry.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/retry.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/retry.ts",
    "build": "tsc",
    "postbuild": "copyfiles package.json task.json dist && cd dist && npm install --only=production",
    "update:version": "ts-node updateVersion.ts --version 0.0.1",
    "create:vsix": "npm run build && npm run update:version && tfx extension create --manifest-globs vss-extension.json --output-path dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.5",
    "azure-pipelines-task-lib": "^3.1.2",
    "glob": "^7.1.6",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.1",
    "@types/minimist": "^1.2.1",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.6.0",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
